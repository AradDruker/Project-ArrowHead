; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Project V"
run/main_scene="res://Scenes/TitleScreen/TitleScreen.tscn"
config/icon="res://Assets/icons/icon.png"

[audio]

default_bus_layout="res://Integrations/default_bus_layout.tres"

[autoload]

GooglePlay="res://Integrations/GooglePlay.gd"
Global="*res://Integrations/global.gd"
Music="*res://Scenes/Music/Music.tscn"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="viewport"

[input_devices]

pointing/emulate_mouse_from_touch=false

[layer_names]

2d_render/layer_1="Borders"
2d_render/layer_2="Player"
2d_render/layer_3="Enemy"
2d_render/layer_4="Coins"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.14902, 0.192157, 0.219608, 1 )
environment/default_environment="res://Integrations/default_env.tres"
quality/dynamic_fonts/use_oversampling=false
